{
  "name": "companion3-prototype",
  "version": "0.1.0",
  "description": "",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "node index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "type-check-client": "tsc -p ./src/client/tsconfig.json",
    "build-client": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack -p",
    "build-server": "tsc -b src",
    "dev-client": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server -w",
    "dev-server": "TS_NODE_PROJECT=src/server/tsconfig.json nodemon src/server/main.ts --ignore config.json",
    "dev": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"yarn dev-server\" \"yarn dev-client\"",
    "build": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"yarn build-server\" \"yarn build-client\""
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gilamran/fullstack-typescript.git"
  },
  "license": "MIT",
  "author": "Gil Amran",
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/src/client/tsconfig-for-tests.json"
      }
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-pouchdb": "^4.2.0",
    "find-up": "^4.1.0",
    "pouchdb-adapter-memory": "^7.1.1",
    "rxdb": "^8.7.5",
    "rxjs": "^6.5.4",
    "socket.io": "^2.3.0",
    "threadedclass": "^0.7.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.18",
    "@types/pouchdb-adapter-http": "^6.1.3",
    "@types/pouchdb-adapter-idb": "^6.1.3",
    "@types/pouchdb-adapter-memory": "^6.1.3",
    "@types/react": "^16.9.19",
    "@types/react-bootstrap": "^0.32.20",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/webpack": "^4.41.3",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-dev-server": "^3.9.0",
    "@types/webpack-manifest-plugin": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "bootstrap": "^4.4.1",
    "bootswatch": "^4.4.1",
    "concurrently": "^5.1.0",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^5.0.2",
    "http-proxy-middleware": "^0.20.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "pouchdb-adapter-http": "^7.1.1",
    "pouchdb-adapter-idb": "^7.1.1",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-contextmenu": "^2.13.0",
    "react-dom": "^16.12.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "react-selectable-fast": "^3.1.1",
    "request": "^2.88.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.2",
    "socket.io-client": "^2.3.0",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
